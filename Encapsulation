public class Encap{
  private String name;
//getter method
public String getName(){return name;}

//Setter Methoda
public void setName(String name){this.name=name;}

public static void main(String[] args){
Encap obj=new Encap();
obj.setName("Nigel");

}



public class Car{
private String brand;
private String model;
private int year;

public String getBrand(){return brand;}
public String getModel(){return model;}
public int getYear(){return year;}

public void setTer(String model, String brand, int year ){

this.brand=brand;
this.model=model;
if(year>0){this.year=year;}
}
public static void main(String[] arg)
{
Car objcar= new Car();
objcar.setTer("Toyo","Yaris",2011);
System.out.println("Brand: "+objcar.getBrand());
System.out.prinlt("Model: "+objcar.getMode());
System.out.prinlt("Model: "+objcar.getYear());
System.out.prinlt("Model: "+objcar.getYear());
}

public class employees{
private String empname;
private int empid;
private int salary;

public String getempname(){
return empname;
}
public int getempid(){return empid;}
public int getsalary(){return salary;}
public void setempname(String empname){ this.empnname=empname;}
public void setempid(int empid){this.empid=empid;}
public void setempsalary(int salary){this.salary=salary;}
public static void main(String[] arg){employees empobj=new employees();
empobj.setempname("Zishan");
empobj.setempid(011);
empobj.setempsalary(12000);
System.out.println("Name: "+empobj.setempname());
System.out.println("empID: "+empobj.setempid());
System.out.println("Salary: "+empobj.setempsalary());
}
}
